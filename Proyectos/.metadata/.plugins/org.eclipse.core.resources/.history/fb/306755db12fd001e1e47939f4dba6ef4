/* Includes ------------------------------------------------------------------*/
#include <FINITE_STATE_MACHINE.h>
#include "main.h"

int main(void) {

	/* Reset of all peripherals, Initializes the Flash interface and the Systick. */
	HAL_Init();

	/* Initialize peripherals */
	SystemFSM_init();

	/* Infinite loop */
	while (1) {
		SystemFSM_update();
	}
}
